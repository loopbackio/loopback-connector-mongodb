{
  "name": "loopback-connector-mongodb",
  "version": "5.6.0",
  "publishConfig": {
    "tag": "lts"
  },
  "description": "The official MongoDB connector for the LoopBack framework.",
  "engines": {
    "node": ">=10"
  },
  "author": "IBM Corp.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "benchmarks": "make benchmarks",
