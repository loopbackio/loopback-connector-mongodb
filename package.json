    "test": "npm run test:ci",
    "test:ci": "nyc mocha test/*.test.js node_modules/juggler-v3/test.js node_modules/juggler-v4/test.js",
    "lint": "eslint .",
    "posttest": "npm run lint"
  },
  "files": [
    "intl",
    "lib",
    "docs.json",
    "index.js",
    "setup.sh"
